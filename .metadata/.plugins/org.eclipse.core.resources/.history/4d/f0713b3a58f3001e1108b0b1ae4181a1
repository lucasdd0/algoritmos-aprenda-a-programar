package exercicios;
import java.util.Scanner;
public class exercicio29 {
	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		
		int n, v, A, achei, i;
		
		System.out.println("Digite a quantidade de elemetos que deseja inserir: ");
		n = input.nextInt();
		
		int v [] = new int [n - 1];
		
		for (i = 0; i < n; i++)
		{
			System.out.println("Digite o " + (i + 1) + "º número: ");
			v[i] = input.nextInt();	
		}
		
		while (i < n && achei == 0)
		{
			if (A[i] = v)
			{
				achei = 1;
			}
			else
			{
				i = i + 1;
			}
		}
		
		
		
		
		input.close();
	}

}
